{
  "expo": {
    "name": "TraningTools",
    "slug": "TraningTools",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon(1).png",
    "scheme": "traningtools",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios"],
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sebastianblazejewski.treningtools",
      "buildNumber": "2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-audio"
    ],
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
